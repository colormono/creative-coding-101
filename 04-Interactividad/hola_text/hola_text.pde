void setup() {
  size(400, 400);  // Define el tamaño de la ventana
}

void draw() {
  background(000); // Establece el color de fondo en negro
  textSize(32);    // Define el tamaño de la fuente
  fill(255);         // Establece el color de fuente en blanco
  textAlign(CENTER, CENTER); // Alinea el texto en el centro
  text("Hola mundo!", width/2, height/2); // Dibuja el texto en el centro
}

